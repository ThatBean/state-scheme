{
  "name": "state-scheme",
  "version": "0.0.5",
  "author": "thatbean",
  "license": "MIT",
  "description": "Create composable State - Reducer by define them as schemes",
  "keywords": [
    "state",
    "scheme",
    "reducer",
    "redux"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thatbean/state-scheme.git"
  },
  "main": "library/index.js",
  "scripts": {
    "example": "cross-env NODE_ENV=development webpack --watch --config config/example.conf.js",
    "library": "cross-env NODE_ENV=production webpack --config config/library.conf.js",
    "build": "npm run library",
    "start": "npm run example",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.1",
    "eslint": "^4.2.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "webpack": "^3.3.0"
  }
}
